<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <add key="CorreoPadreES" value="asistencia@generad.net"/>
    <add key="CorreoPadreEN" value="assistance@generad.net"/>
    <add key="contrseñaPadre" value="supj23533589"/>
    <!--gmail-->
    <!--<add key="host" value="smtp.gmail.com"/>
    <add key="port" value="587"/>-->
    <!--outlook-->
    <add key="host" value="smtpout.secureserver.net"/>
    <add key="port" value="587"/>
  </appSettings>
  <system.net>
    <mailSettings>
      <smtp from="asistencia@generad.net">
        <network host="smtpout.secureserver.net" port="465" />
      </smtp>
    </mailSettings>
  </system.net>
  <system.web>
    <trust level="Full"/>
    <customErrors mode="Off"/>
    <compilation debug="true" targetFramework="4.7.2"/>
  </system.web>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Force HTTPS" enabled="true">
          <match url="(.*)" ignoreCase="false"/>
          <conditions>
            <add input="{HTTPS}" pattern="off"/>
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}/{R:1}" appendQueryString="true" redirectType="Permanent"/>
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>