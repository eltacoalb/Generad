
@{
    ViewBag.Title = "Index";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="css/estilos.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js"></script>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <link href="~/css/estilos.css" rel="stylesheet" type="text/css" media="all" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">

    <script src="~/js/jquery-2.1.4.min.js"></script>
    <script src="~/js/move-top.js"></script>
    <script src="~/js/slick.js"></script>
    <title>Genera-D</title>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar navbar-dark bg-dark">
        <a class="navbar-brand" href="index.html"><img src="~/img/GENERA%20D,HOLDING,GOTA%20DE%20AGUA,MEDIA%20CARTA,130519.png" class="tam_logo" alt=""></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class=" pst" id="navbarNavDropdown">
            <ul class="navbar-nav">
                <li class="nav-item ">
                    <a class="nav-link active" href="@Url.Action("Index", "GeneraD", new { lang = Resources.Menu.lang })">@Resources.Menu.Inicio<span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Nosotros", "GeneraD", new { lang = Resources.Menu.lang })">@Resources.Menu.Nosotros</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Holding", "GeneraD", new { lang = Resources.Menu.lang })">@Resources.Menu.Holding</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Comercializacion", "GeneraD", new { lang = Resources.Menu.lang })">@Resources.Menu.Comercializacion</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("IntegracionPersonal", "GeneraD", new { lang = Resources.Menu.lang })">@Resources.Menu.Integracionpersonal</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Documental", "GeneraD", new { lang = Resources.Menu.lang })">@Resources.Menu.Documental</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Contacto", "GeneraD", new { lang = Resources.Menu.lang })">@Resources.Menu.Contacto</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("ChangeLanguage", "GeneraD", new { lang = Resources.Menu.lang })">@Resources.Menu.Idioma</a>
                </li>

            </ul>
        </div>
    </nav>
    <section class="inicio">
        <div class="caja_texto">
            <span class="titulo1">@Resources.Inicio.Presentacion_subtitulo</span>
            <span class="titulo2">@Resources.Inicio.Presentacion_eslogan</span>
            <div class="c_botones">
                <button class="boton1">@Resources.Inicio.Presentacion_llave</button>
                <button class="boton2 post">@Resources.Inicio.Presentacion_conector</button>
            </div>

        </div>

    </section>
    <section class="seccion2">
        <span class="t_sec1">@Resources.Inicio.Lo_que_hacemos</span>
        <div class="c_img">
            <div class="c1p">
                <img src="~/img/lucro.png" alt="" class="impt">
                <p class="p1i">
                    @Resources.Inicio.Lo_que_hacemos_1
                </p>
            </div>
            <div class="c2p">
                <img src="~/img/top-tres.png" alt="" class="impt">
                <p class="p1i">
                    @Resources.Inicio.Lo_que_hacemos_2
                </p>
            </div>
            <div class="c3p">
                <img src="~/img/rompecabezas.png" alt="" class="impt">
                <p class="p1i">
                    @Resources.Inicio.Lo_que_hacemos_3
                </p>
            </div>
        </div>
    </section>
    <section class="seccion2">
        <span class="t_sec2">@Resources.Inicio.Como_lo_hacemos</span>
        <div class="c_img">
            <div class="testimonials">
                <div class="container">
                    <div class="phpkida_testimonials_grids">
                        <section class="center slider">
                            <div class="agileits_testimonial_grid">
                                <div class="pk_testimonial_grid">
                                    <p>@Resources.Inicio.Como_lo_hacemos_1</p>
                                    <div class="pk_testimonial_grid_pos">

                                    </div>
                                </div>
                            </div>
                            <div class="agileits_testimonial_grid">
                                <div class="pk_testimonial_grid">
                                    <p>@Resources.Inicio.Como_lo_hacemos_2</p>


                                </div>
                            </div>
                            <div class="agileits_testimonial_grid">
                                <div class="pk_testimonial_grid">
                                    <p>@Resources.Inicio.Como_lo_hacemos_3</p>

                                    <div class="pk_testimonial_grid_pos">

                                    </div>
                                </div>
                            </div>
                            <div class="agileits_testimonial_grid">
                                <div class="pk_testimonial_grid">
                                    <p>@Resources.Inicio.Como_lo_hacemos_4</p>

                                    <div class="pk_testimonial_grid_pos">

                                    </div>
                                </div>
                            </div>
                            <div class="agileits_testimonial_grid">
                                <div class="pk_testimonial_grid">
                                    <p>@Resources.Inicio.Como_lo_hacemos_5</p>

                                    <div class="pk_testimonial_grid_pos">

                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </div>
            </div>
            <!-- //testimonials -->
            <!-- carousal -->

            <script src="js/slick.js" type="text/javascript" charset="utf-8"></script>
            <script type="text/javascript">
	$(document).on('ready', function() {
	  $(".center").slick({
		dots: true,
		infinite: true,
		centerMode: true,
		slidesToShow: 2,
		slidesToScroll: 2,
		responsive: [
			{
			  breakpoint: 768,
			  settings: {
				arrows: true,
				centerMode: false,
				slidesToShow: 2
			  }
			},
			{
			  breakpoint: 480,
			  settings: {
				arrows: true,
				centerMode: false,
				centerPadding: '40px',
				slidesToShow: 1
			  }
			}
		 ]
	  });
	});
            </script>
            <!-- //carousal -->
            <!-- start-smooth-scrolling -->
            <script type="text/javascript" src="js/move-top.js"></script>


        </div>
    </section>


    <br>
    <br>
    <br>
    <br>
    <br>
    <br>

    <seccion class="scn1">
        <div class="scnn1">

            <div class="blog-card">
                <input type="radio" name="select" id="tap-1" checked>
                <input type="radio" name="select" id="tap-2">
                <input type="radio" name="select" id="tap-3">
                <input type="checkbox" id="imgTap">
                <div class="sliders">
                    <label for="tap-1" class="tap tap-1"></label>
                    <label for="tap-2" class="tap tap-2"></label>
                    <label for="tap-3" class="tap tap-3"></label>
                </div>
                <div class="inner-part">
                    <label for="imgTap" class="img">
                        <img class="img-1" src="#">
                    </label>
                    <div class="content content-1">
                        <span class="111">@Resources.Inicio.Holding</span>

                        <div class="text">
                            @Resources.Inicio.Holding_1 <br>
                            @Resources.Inicio.Holding_2
                            <br>
                            @Resources.Inicio.Holding_3
                        </div>
                        <button>@Resources.Inicio.Presentacion_llave</button>
                    </div>
                </div>
                <div class="inner-part">
                    <label for="imgTap" class="img">
                        <img class="img-2" src="#">
                    </label>
                    <div class="content content-2">
                        <span>@Resources.Inicio.Comercializacion</span>

                        <div class="text">
                            @Resources.Inicio.Comercializacion<br>
                            <br>
                            @Resources.Inicio.Comercializacion_2 <br>
                        </div>
                        <button>@Resources.Inicio.Presentacion_llave</button>
                    </div>
                </div>
                <div class="inner-part">
                    <label for="imgTap" class="img">
                        <img class="img-3" src="#">
                    </label>
                    <div class="content content-3">
                        <span>@Resources.Inicio.Integracion_personal</span>

                        <div class="text">
                            @Resources.Inicio.Integracion_personal_1
                        </div>
                        <button>@Resources.Inicio.Presentacion_llave</button>
                    </div>
                </div>
            </div>



        </div>
    </seccion>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <section class="contacto">

        <div class="c_contacto c_prin">
            <span class="text1">
                @Resources.Contacto_div.Contacto
                <br>  @Resources.Contacto_div.Contacto_2
                <br>  @Resources.Contacto_div.Contacto_5
            <span class="text2"> <br>  @Resources.Contacto_div.Contacto_1</span>
                <br>
                <span class="text2"> <br> @Resources.Contacto_div.Contacto_3</span>
                <br>
                <span class="text2"> <br> @Resources.Contacto_div.Contacto_4</span>

            </span>

        </div>

    </section>
    <section class="footer ">
    </section>
</body>
</html>