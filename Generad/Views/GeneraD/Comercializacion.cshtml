                        <h4 class="title"><a href="">@Resources.Comercializacion.equipo_medico</a></h4>
                        <p class="description">@Resources.Comercializacion.equipo_medico_1</p>
                    </div>
                </div>

                <div class="col-md-6 col-lg-3 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="100">
                    <div class="icon-box icon-box-cyan">
                        <div class="icon"><img src="data:image/svg+xml;base64," style="width: 66%;height: 80%;" /></div>
                        <h4 class="title"><a href="">@Resources.Comercializacion.maquinaria</a></h4>
                        <p class="description">@Resources.Comercializacion.maquinaria_1</p>
                    </div>
                </div>

                <div class="col-md-6 col-lg-3 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="200">
                    <div class="icon-box icon-box-green">
                        <div class="icon"><img src="data:image/svg+xml;base64," style="width: 66%;height: 80%;" /></div>
                        <h4 class="title"><a href="">@Resources.Comercializacion.productos_sustentables</a></h4>
                        <p class="description">@Resources.Comercializacion.productos_sustentables_1</p>
                    </div>
                </div>

                <div class="col-md-6 col-lg-3 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="200">
                    <div class="icon-box icon-box-blue">
                        <div class="icon"><img src="data:image/svg+xml;base64," style="width: 66%;height: 80%;" /></div>
                        <h4 class="title"><a href="">@Resources.Comercializacion.commodities</a></h4>
                        <p class="description">@Resources.Comercializacion.commodities_1</p>
                    </div>
                </div>

            </div>
        </div>
    </section><!-- End Services Section -->
    <!-- ======= About Section ======= -->

    <section class="about" data-aos="fade-up">
        <div class="container">
            <div class="section-title">
                <h2>@Resources.Comercializacion.miel</h2>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <center>
                        <img src="~/Content/img/generad/COMERSIALIZACIÓN 06,MIEL,MAPA PRODUCTORES.jpg" class="img-fluid" alt="" style="max-width:75%!important;" />
                    </center>
                </div>
                <div class="col-lg-6 pt-4 pt-lg-0">
                    <h3>@Resources.Comercializacion.la_miel_en_el_mundo</h3>
                    <p>
                        @Resources.Comercializacion.la_miel_en_el_mundo_1
                    </p>

                </div>
                <p>
                    @Resources.Comercializacion.la_miel_en_el_mundo_2
