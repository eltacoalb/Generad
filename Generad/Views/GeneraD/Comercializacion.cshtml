    ======================================================== -->
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top d-flex align-items-center ">
        @Html.Partial("_MenuBar")
    </header><!-- End Header -->

    <main id="main">

        <!-- ======= About Us Section ======= -->
        <section class="breadcrumbs">
            <div class="container">

                <div class="d-flex justify-content-between align-items-center">
                    <h2>@Resources.Menu.Comercializacion</h2>
                </div>

            </div>
        </section><!-- End About Us Section -->
        <!-- ======= Services Section ======= -->
        <section class="services">
            <div class="container">
                <div class="section-title">
                    <h2>@Resources.Comercializacion.productos_y_servicios</h2>
                </div>

                <div class="row">
                    <div class="col-md-6 col-lg-4 d-flex align-items-stretch" data-aos="fade-up">
                        <div class="icon-box icon-box-pink">
                            <div class="icon"><img src="data:image/svg+xml;base64," style="width: 66%;height: 80%;" /></div>
                            <h4 class="title"><a href="">@Resources.Comercializacion.equipo_medico</a></h4>
                            <p class="description">@Resources.Comercializacion.equipo_medico_1</p>
                        </div>
                    </div>

                    <div class="col-md-6 col-lg-4 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="100">
                        <div class="icon-box icon-box-cyan">
                            <div class="icon"><img src="data:image/svg+xml;base64," style="width: 66%;height: 80%;" /></div>
                            <h4 class="title"><a href="">@Resources.Comercializacion.maquinaria</a></h4>
                            <p class="description">@Resources.Comercializacion.maquinaria_1</p>
                        </div>
                    </div>

                    <div class="col-md-6 col-lg-4 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="200">
                        <div class="icon-box icon-box-green">
                            <div class="icon"><img src="data:image/svg+xml;base64," style="width: 66%;height: 80%;" /></div>
                            <h4 class="title"><a href="">@Resources.Comercializacion.productos_sustentables</a></h4>
                            <p class="description">@Resources.Comercializacion.productos_sustentables_1</p>
                        </div>
                    </div>
                </div>

            </div>
        </section><!-- End Services Section -->
        <!-- ======= About Section ======= -->
        <section class="about" data-aos="fade-up">
            <div class="container">
                <div class="section-title">
                    <h2>@Resources.Comercializacion.miel</h2>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <img src="~/Content/img/generad/COMERSIALIZACIÓN 06,MIEL,MAPA PRODUCTORES.jpg" class="img-fluid" alt="" />
                    </div>
                    <div class="col-lg-6 pt-4 pt-lg-0">
                        <h3>@Resources.Comercializacion.la_miel_en_el_mundo</h3>
                        <p>
                            @Resources.Comercializacion.la_miel_en_el_mundo_1
                        </p>
                        <p>
                            @Resources.Comercializacion.la_miel_en_el_mundo_2
                        </p>
                    </div>
                </div>

            </div>
        </section><!-- End About Section -->
        <!-- ======= Features Section ======= -->
        <section class="features">
            <div class="container">

                <div class="section-title">
                    <h2>@Resources.Comercializacion.miel_mexicana_uniflora</h2>
                </div>

                <div class="row" data-aos="fade-up">
                    <div class="pt-4">
                        <p>
                            @Resources.Comercializacion.miel_mexicana_uniflora_1
                        </p>
                        <p>
                            @Resources.Comercializacion.miel_mexicana_uniflora_2
                        </p>
                        <p>
                            @Resources.Comercializacion.miel_mexicana_uniflora_3
                        </p>
                    </div>
                </div>
                <br />
                <br />
                <div class="section-title">
                    <h2>@Resources.Comercializacion.miel_mexicana_multiflora</h2>
                </div>

                <div class="row" data-aos="fade-up">
                    <div class="pt-5 order-2 order-md-1">
                        <p>
                            @Resources.Comercializacion.miel_mexicana_multiflora_1
                        </p>
                    </div>
                </div>

                <br />
                <br />
                <div class="section-title">
                    <h2>@Resources.Comercializacion.fuerza_productora</h2>
                </div>

                <div class="row" data-aos="fade-up">
                    <div class="pt-5">
                        <p>@Resources.Comercializacion.fuerza_productora_1</p>
                    </div>
                </div>
            </div>
        </section><!-- End Features Section -->
        <section>
            <div class="container">
                <div class="row">
                    <div class="col"><a href="#step1" class="btn btn-secondary redondo" data-toggle="collapse">@Resources.Comercializacion.proceso_de_venta_paso1</a></div>
                    <div class="col"><a href="#step2" class="btn btn-secondary redondo" data-toggle="collapse">@Resources.Comercializacion.proceso_de_venta_paso2</a></div>
                    <div class="col"><a href="#step3" class="btn btn-secondary redondo" data-toggle="collapse">@Resources.Comercializacion.proceso_de_venta_paso3</a></div>
                    <div class="col"><a href="#step4" class="btn btn-secondary redondo" data-toggle="collapse">@Resources.Comercializacion.proceso_de_venta_paso4</a></div>
                   
                </div>
                <br />
                <div class="row">
                    <ul>
                        <li id="step1" class="bi collapse">
                            <div>
                                <p>@Resources.Comercializacion.proceso_de_venta_paso1_1</p>
                            </div>
                        </li>
                        <li id="step2" class="bi collapse">
                            <div>
                                <p>@Resources.Comercializacion.proceso_de_venta_paso2_1</p>
                            </div>
                        </li>
                        <li id="step3" class="bi collapse">
                            <div>
                                <p>@Resources.Comercializacion.proceso_de_venta_paso3_1</p>
                            </div>
                        </li>
                        <li id="step4" class="bi collapse">
                            <div>
                                <p>@Resources.Comercializacion.proceso_de_venta_paso4_1</p>
                            </div>
                        </li>
                    </ul>
                   
                    
                    
                    
                </div>
            </div>
        </section>
    </main><!-- End #main -->
    <!-- ======= Footer ======= -->
    <footer id="footer" data-aos="fade-up" data-aos-easing="ease-in-out" data-aos-duration="500">

        <div class="footer-top">
            @Html.Partial("_Footer")
        </div>

        <div class="container">
            <div class="copyright">
                &copy; Copyright <strong><span>Moderna</span></strong>. All Rights Reserved
            </div>
            <div class="credits">
                <!-- All the links in the footer should remain intact. -->
                <!-- You can delete the links only if you purchased the pro version. -->
                <!-- Licensing information: https://bootstrapmade.com/license/ -->
                <!-- Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/free-bootstrap-template-corporate-moderna/ -->
                Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
            </div>
        </div>
    </footer><!-- End Footer -->

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/Content/vendor/aos/aos.js"></script>
    <script src="~/Content/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/Content/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/Content/vendor/php-email-form/validate.js"></script>
    <script src="~/Content/vendor/purecounter/purecounter.js"></script>
    <script src="~/Content/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/Content/vendor/waypoints/noframework.waypoints.js"></script>
    <style>
        .redondo {
            display: block;
            width: 200px;
            height: 65px;
            border-radius: 180px;
            background:#68A4C4;
        }
    </style>
    <!-- Template Main JS File -->
    <script src="~/Scripts/js/main.js"></script>

</body>

</html>