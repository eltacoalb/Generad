                    </p>
                </div>
            </div>
            <br />
            <br />
            <div class="section-title">
                <h2>@Resources.Comercializacion.fuerza_productora</h2>
            </div>
            <div class="row" data-aos="fade-up">
                <div class="col-md-5">
                    <img src="~/Content/img/generad/FUERZA PRODUCTORA apicultores.jpeg" class="img-fluid" />
                </div>
                <div class="col-md-7 pt-4">
                    <p>@Resources.Comercializacion.fuerza_productora_1</p>
                </div>
            </div>
        </div>
    </section><!-- End Features Section -->
    <section>
        <div class="container">
            <center>
                <div class="row">
                    <div class="col-sm-3"><a href="#step1" class="btn btn-secondary redondo btnStep" data-toggle="collapse">@Resources.Comercializacion.proceso_de_venta_paso1</a></div>
                    <div class="col-sm-3"><a href="#step2" class="btn btn-secondary redondo btnStep" data-toggle="collapse">@Resources.Comercializacion.proceso_de_venta_paso2</a></div>
                    <div class="col-sm-3"><a href="#step3" class="btn btn-secondary redondo btnStep" data-toggle="collapse">@Resources.Comercializacion.proceso_de_venta_paso3</a></div>
                    <div class="col-sm-3"><a href="#step4" class="btn btn-secondary redondo btnStep" data-toggle="collapse">@Resources.Comercializacion.proceso_de_venta_paso4</a></div>
                </div>
            </center>
            <br />
            <div class="row">
                <center>
                    <ul class="no-bullets">
                        <li id="step1" class="bi collapse">
                            <div>
                                <p>@Resources.Comercializacion.proceso_de_venta_paso1_1 <a href="#myModal" data-toggle="modal"><img src="~/Content/img/generad/LogoBejamel.png" style="max-width:120px;max-height:120px" title="@Resources.Comercializacion.title" /></a></p>


                            </div>
                        </li>
                        <li id="step2" class="bi collapse">
                            <div>
                                <p>@Resources.Comercializacion.proceso_de_venta_paso2_1</p>
                            </div>
                        </li>
                        <li id="step3" class="bi collapse">
                            <div>
                                <p>@Resources.Comercializacion.proceso_de_venta_paso3_1</p>
                            </div>
                        </li>
                        <li id="step4" class="bi collapse">
                            <div>
                                <p>@Resources.Comercializacion.proceso_de_venta_paso4_1</p>
                            </div>
                        </li>
                    </ul>
                </center>
            </div>
        </div>
    </section>
</main><!-- End #main -->
<!-- The Modal -->
<div class="modal fade" id="myModal">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">

            <div id="demo" class="carousel slide" data-ride="carousel">
                <ul class="carousel-indicators">

                    @for (int i = 0; i < Model.ImagesCount; i++)
                    {
                        if (i == 0)
                        {
                            <li data-target="#demo" data-slide-to="@i" class="active"></li>
                        }
                        else
                        {
                            <li data-target="#demo" data-slide-to="@i"></li>
                        }
                    }

                </ul>
                <div class="carousel-inner">
                    @for (int j = 0; j < Model.ImagesName.Count; j++)
                    {
                        if (j == 0)
                        {
                            <div class="carousel-item active">
                                <img src="~/Content/img/generad/Productos/@Model.ImagesName[j]" width="1100" height="500" />
                                @*<div class="carousel-caption">
                                    <h3>Los Angeles</h3>
                                    <p>We had such a great time in LA!</p>
                                    </div>*@
                            </div>
                        }
                        else
                        {
                            <div class="carousel-item">
                                <img src="~/Content/img/generad/Productos/@Model.ImagesName[j]" width="1100" height="500" />
                                @*<div class="carousel-caption">
                                        <h3>Chicago</h3>
                                        <p>Thank you, Chicago!</p>
                                    </div>*@
                            </div>
                        }
                    }
                </div>
                <a class="carousel-control-prev" href="#demo" data-slide="prev">
                    <span class="carousel-control-prev-icon" style="background-color:black;"></span>
                </a>
                <a class="carousel-control-next" href="#demo" data-slide="next">
                    <span class="carousel-control-next-icon" style="background-color:black;"></span>
                </a>
            </div>

        </div>
    </div>
</div>

<style>
    .redondo {
        display: flex;
        width: 220px;
        height: 65px;
        border-radius: 180px;
        background: #68A4C4;
    }

    ul.no-bullets {
        list-style-type: none;
        margin: 0;
        padding: 0;
    }

    .btnStep {
        display: inline-flex;
        justify-content: center; /* center the content horizontally */
        align-items: center; /* center the content vertically */
        --padding-x: 1.2em;
        border-color: transparent; /* hide button border */
    }

    .carousel-inner img {
        width: 100%;
        height: 100%;
    }
</style>
