@using Generad.Models
@model CatalogoModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<main id="main">

    <!-- ======= About Us Section ======= -->
    <section class="breadcrumbs">
        <div class="container">

            <div class="d-flex justify-content-between align-items-center">
